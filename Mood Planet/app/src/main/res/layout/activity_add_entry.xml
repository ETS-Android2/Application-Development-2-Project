<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bg"
    tools:context=".AddEntryActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="44dp"
        android:fontFamily="casual"
        android:text="New Mood Entry"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="268dp"
        android:layout_marginEnd="96dp"
        android:fontFamily="casual"
        android:text="How do you feel today?"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageButton
        android:id="@+id/calmIB"
        android:layout_width="132dp"
        android:layout_height="145dp"
        android:layout_marginTop="148dp"
        android:layout_marginEnd="260dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/calm"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/heheIB"
        android:layout_width="201dp"
        android:layout_height="194dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="280dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@mipmap/hehehe"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/hmmIB"
        android:layout_width="167dp"
        android:layout_height="219dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="108dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="@+id/heheIB"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/hmmm"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/loveIB"
        android:layout_width="161dp"
        android:layout_height="178dp"
        android:layout_marginTop="248dp"
        android:layout_marginEnd="276dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/love"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ohnoIB"
        android:layout_width="181dp"
        android:layout_height="202dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="192dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calmIB"
        app:srcCompat="@mipmap/ohno"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/okIB"
        android:layout_width="221dp"
        android:layout_height="241dp"
        android:layout_marginTop="460dp"
        android:layout_marginEnd="4dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ok"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerView6"
        android:name="com.example.moodplanet.MenuFragment"
        android:layout_width="408dp"
        android:layout_height="43dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/sadIB"
        android:layout_width="164dp"
        android:layout_height="196dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="124dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/sad"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/yayIB"
        android:layout_width="202dp"
        android:layout_height="202dp"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="224dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/yay"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/zzzIB"
        android:layout_width="188dp"
        android:layout_height="170dp"
        android:layout_marginStart="252dp"
        android:layout_marginTop="256dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/zzz"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>