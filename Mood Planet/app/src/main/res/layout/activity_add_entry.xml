<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bg"
    tools:context=".AddEntryActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="40dp"
        android:fontFamily="casual"
        android:text="New Mood Entry"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="236dp"
        android:layout_marginEnd="172dp"
        android:fontFamily="casual"
        android:text="How do"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageButton
        android:id="@+id/calmImageButton"
        android:layout_width="114dp"
        android:layout_height="137dp"
        android:layout_marginTop="468dp"
        android:layout_marginEnd="268dp"
        android:backgroundTint="#00D39465"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/calm"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/heheheImageButton"
        android:layout_width="117dp"
        android:layout_height="131dp"
        android:layout_marginStart="296dp"
        android:layout_marginTop="224dp"
        android:backgroundTint="#00D50000"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@mipmap/hehehe"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/hmmImageButton"
        android:layout_width="120dp"
        android:layout_height="121dp"
        android:layout_marginStart="280dp"
        android:layout_marginTop="172dp"
        android:backgroundTint="#009E2323"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/hmmm"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/loveImageButton"
        android:layout_width="118dp"
        android:layout_height="130dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="268dp"
        android:backgroundTint="#0067F105"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/love"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="108dp"
        android:layout_height="123dp"
        android:layout_marginStart="208dp"
        android:layout_marginTop="180dp"
        android:backgroundTint="#00FB1C1C"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calmImageButton"
        app:srcCompat="@mipmap/ohno"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/okImageButton"
        android:layout_width="120dp"
        android:layout_height="129dp"
        android:layout_marginTop="516dp"
        android:layout_marginEnd="132dp"
        android:backgroundTint="#00B10B0B"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ok"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerView6"
        android:name="com.example.moodplanet.MenuFragment"
        android:layout_width="408dp"
        android:layout_height="43dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/sadImageButton"
        android:layout_width="104dp"
        android:layout_height="123dp"
        android:layout_marginStart="296dp"
        android:layout_marginTop="468dp"
        android:backgroundTint="#0000BCD4"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/sad"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/yayImageButton"
        android:layout_width="117dp"
        android:layout_height="141dp"
        android:layout_marginTop="320dp"
        android:layout_marginEnd="276dp"
        android:backgroundTint="#00CDDC39"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/yay"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/zzzImageButton"
        android:layout_width="123dp"
        android:layout_height="131dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="124dp"
        android:backgroundTint="#00CDDC39"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/zzz"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="32dp"
        android:fontFamily="casual"
        android:text="you feel today?"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

</androidx.constraintlayout.widget.ConstraintLayout>